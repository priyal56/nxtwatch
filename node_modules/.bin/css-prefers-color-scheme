#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/css-prefers-color-scheme@6.0.3_postcss@8.4.35/node_modules/css-prefers-color-scheme/dist/cli.cjs/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/css-prefers-color-scheme@6.0.3_postcss@8.4.35/node_modules/css-prefers-color-scheme/dist/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/css-prefers-color-scheme@6.0.3_postcss@8.4.35/node_modules/css-prefers-color-scheme/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/css-prefers-color-scheme@6.0.3_postcss@8.4.35/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/css-prefers-color-scheme/dist/cli.cjs/node_modules:/home/nxtwave/nxtwatch/node_modules/css-prefers-color-scheme/dist/node_modules:/home/nxtwave/nxtwatch/node_modules/css-prefers-color-scheme/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../css-prefers-color-scheme/dist/cli.cjs" "$@"
else 
  exec node  "$basedir/../css-prefers-color-scheme/dist/cli.cjs" "$@"
fi
