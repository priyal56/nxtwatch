#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/terser@5.27.0/node_modules/terser/bin/terser/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/terser@5.27.0/node_modules/terser/bin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/terser@5.27.0/node_modules/terser/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/terser@5.27.0/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/terser/bin/terser/node_modules:/home/nxtwave/nxtwatch/node_modules/terser/bin/node_modules:/home/nxtwave/nxtwatch/node_modules/terser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else 
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
