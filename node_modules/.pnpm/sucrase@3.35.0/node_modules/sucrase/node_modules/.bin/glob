#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/glob@10.3.10/node_modules/glob/dist/esm/bin.mjs/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/glob@10.3.10/node_modules/glob/dist/esm/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/glob@10.3.10/node_modules/glob/dist/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/glob@10.3.10/node_modules/glob/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/glob@10.3.10/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/sucrase@3.35.0/node_modules/sucrase/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/sucrase@3.35.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../glob@10.3.10/node_modules/glob/dist/esm/bin.mjs" "$@"
else 
  exec node  "$basedir/../../../../../glob@10.3.10/node_modules/glob/dist/esm/bin.mjs" "$@"
fi
