#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.90.1/node_modules/webpack-dev-server/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.90.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html" "$@"
else 
  exec node  "$basedir/../../../../../ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html" "$@"
fi
