#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/browserslist@4.23.0/node_modules/browserslist/cli.js/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/browserslist@4.23.0/node_modules/browserslist/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/browserslist@4.23.0/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/stylehacks@5.1.1_postcss@8.4.35/node_modules/stylehacks/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/stylehacks@5.1.1_postcss@8.4.35/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.23.0/node_modules/browserslist/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../browserslist@4.23.0/node_modules/browserslist/cli.js" "$@"
fi
