#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules/webpack/bin/webpack.js/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules/webpack/bin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules/webpack/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@pmmmwh/react-refresh-webpack-plugin@0.5.11_3d2c163a1499bd98816f225623c9a77d/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@pmmmwh/react-refresh-webpack-plugin@0.5.11_3d2c163a1499bd98816f225623c9a77d/node_modules/@pmmmwh/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@pmmmwh/react-refresh-webpack-plugin@0.5.11_3d2c163a1499bd98816f225623c9a77d/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@pmmmwh/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../webpack@5.90.1/node_modules/webpack/bin/webpack.js" "$@"
else 
  exec node  "$basedir/../../../../../../../webpack@5.90.1/node_modules/webpack/bin/webpack.js" "$@"
fi
