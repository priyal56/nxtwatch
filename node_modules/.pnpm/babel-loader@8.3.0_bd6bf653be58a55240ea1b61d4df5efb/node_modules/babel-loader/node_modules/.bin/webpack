#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules/webpack/bin/webpack.js/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules/webpack/bin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules/webpack/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/babel-loader@8.3.0_bd6bf653be58a55240ea1b61d4df5efb/node_modules/babel-loader/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/babel-loader@8.3.0_bd6bf653be58a55240ea1b61d4df5efb/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@5.90.1/node_modules/webpack/bin/webpack.js" "$@"
else 
  exec node  "$basedir/../../../../../webpack@5.90.1/node_modules/webpack/bin/webpack.js" "$@"
fi
