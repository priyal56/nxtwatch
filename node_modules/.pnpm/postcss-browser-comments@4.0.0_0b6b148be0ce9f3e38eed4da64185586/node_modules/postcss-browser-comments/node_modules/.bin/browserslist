#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/browserslist@4.23.0/node_modules/browserslist/cli.js/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/browserslist@4.23.0/node_modules/browserslist/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/browserslist@4.23.0/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/postcss-browser-comments@4.0.0_0b6b148be0ce9f3e38eed4da64185586/node_modules/postcss-browser-comments/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/postcss-browser-comments@4.0.0_0b6b148be0ce9f3e38eed4da64185586/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.23.0/node_modules/browserslist/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../browserslist@4.23.0/node_modules/browserslist/cli.js" "$@"
fi
