#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules/webpack/bin/webpack.js/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules/webpack/bin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules/webpack/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack@5.90.1/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/mini-css-extract-plugin@2.8.0_webpack@5.90.1/node_modules/mini-css-extract-plugin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/mini-css-extract-plugin@2.8.0_webpack@5.90.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@5.90.1/node_modules/webpack/bin/webpack.js" "$@"
else 
  exec node  "$basedir/../../../../../webpack@5.90.1/node_modules/webpack/bin/webpack.js" "$@"
fi
