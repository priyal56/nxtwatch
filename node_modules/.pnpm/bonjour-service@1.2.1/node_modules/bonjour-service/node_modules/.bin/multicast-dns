#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/multicast-dns@7.2.5/node_modules/multicast-dns/cli.js/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/multicast-dns@7.2.5/node_modules/multicast-dns/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/multicast-dns@7.2.5/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/bonjour-service@1.2.1/node_modules/bonjour-service/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/bonjour-service@1.2.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../multicast-dns@7.2.5/node_modules/multicast-dns/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../multicast-dns@7.2.5/node_modules/multicast-dns/cli.js" "$@"
fi
