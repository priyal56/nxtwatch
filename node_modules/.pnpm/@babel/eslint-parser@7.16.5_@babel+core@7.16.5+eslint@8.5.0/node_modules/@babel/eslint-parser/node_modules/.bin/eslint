#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/bin/eslint.js/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/bin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/eslint@8.5.0/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@babel/eslint-parser@7.16.5_@babel+core@7.16.5+eslint@8.5.0/node_modules/@babel/eslint-parser/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@babel/eslint-parser@7.16.5_@babel+core@7.16.5+eslint@8.5.0/node_modules/@babel/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@babel/eslint-parser@7.16.5_@babel+core@7.16.5+eslint@8.5.0/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@babel/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../eslint@8.5.0/node_modules/eslint/bin/eslint.js" "$@"
else 
  exec node  "$basedir/../../../../../../../eslint@8.5.0/node_modules/eslint/bin/eslint.js" "$@"
fi
