#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/jest@27.5.1/node_modules/jest/bin/jest.js/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/jest@27.5.1/node_modules/jest/bin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/jest@27.5.1/node_modules/jest/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/jest@27.5.1/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/react-scripts@5.0.1_react@18.2.0/node_modules/react-scripts/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/react-scripts@5.0.1_react@18.2.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest@27.5.1/node_modules/jest/bin/jest.js" "$@"
else 
  exec node  "$basedir/../../../../../jest@27.5.1/node_modules/jest/bin/jest.js" "$@"
fi
