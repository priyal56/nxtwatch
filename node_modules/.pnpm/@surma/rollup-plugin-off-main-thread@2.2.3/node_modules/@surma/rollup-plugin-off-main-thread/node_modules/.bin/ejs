#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/ejs@3.1.9/node_modules/ejs/bin/cli.js/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/ejs@3.1.9/node_modules/ejs/bin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/ejs@3.1.9/node_modules/ejs/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/ejs@3.1.9/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@surma/rollup-plugin-off-main-thread@2.2.3/node_modules/@surma/rollup-plugin-off-main-thread/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@surma/rollup-plugin-off-main-thread@2.2.3/node_modules/@surma/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@surma/rollup-plugin-off-main-thread@2.2.3/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@surma/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../ejs@3.1.9/node_modules/ejs/bin/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../../../ejs@3.1.9/node_modules/ejs/bin/cli.js" "$@"
fi
